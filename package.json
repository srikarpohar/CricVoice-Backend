{
  "name": "cricvoice",
  "version": "1.0.0",
  "description": "Package.json file for cricvoice application",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "build": "tsc",
    "dbmigrate-dev": "npx prisma migrate dev",
    "dbgenerate": "npx prisma generate",
    "start": "node --loader ts-node/esm server.ts"
  },
  "prisma": {
    "schema": "./cricvoice-backend/prisma/schema.dev.prisma"
  },
  "nodemonConfig": {
    "watch": [
      "./**/*.ts",
      "server.ts"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.24.0",
    "@types/node": "^15.6.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "fs": "0.0.1-security",
    "fs-extra": "^10.0.0",
    "handy-redis": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "uninstall": "0.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "prisma": "^2.24.0"
  }
}
